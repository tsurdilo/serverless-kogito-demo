{
	"id": "testworkflow",
	"name": "mytestworkflow",
	"version": "1.0",
	"startsAt": "SayHelloState",
	"functions": [{
		"name": "sysoutfunction",
		"type": "sysout"
	}],
	"states": [{
		"name": "SayHelloState",
		"type": "OPERATION",
		"actionMode": "SEQUENTIAL",
		"actions": [{
				"functionref": {
					"refname": "sysoutfunction",
					"parameters": {
						"message": "\"Hello\" + workflowdata.name"
					}
				}
			},
			{
				"functionref": {
					"refname": "sysoutfunction",
					"parameters": {
						"message": "\"Goodbye\" + workflowdata.name"
					}
				}
			}
		],
		"end": {
			"type": "TERMINATE"
		}
	}]


}